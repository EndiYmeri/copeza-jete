---
import BaseHead from "../components/common/BaseHead.astro";
import Header from "../components/Header.astro";
import Footer from "../components/Footer.astro";
import type { MasonryImages } from "../types/image"; // Assuming MasonryImages type

interface Props {
  masonryImages: MasonryImages;
  title: string;
  imageUrls: string[]; // Add the new prop for image URLs
}

const { masonryImages, title, imageUrls } = Astro.props;
---

<html lang="en">
  <head>
    <BaseHead title={title} description={""} imageUrls={imageUrls} />
    {/* Pass imageUrls to BaseHead */}
  </head>
  <body class="bg-bgPrimary">
    <Header />
    {/* Removed duplicate main tag here */}
    <main class="mx-auto px-4 py-8 container">
      <section class="relative"></section>
    </main>
    <Footer />
  </body>
</html>
